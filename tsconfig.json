{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "module": "commonjs",
    "target": "es2019",
    // 为ts自动生成.d.ts声明文件
    "declaration": true,
    "baseUrl": "./",
    "experimentalDecorators": true,
    "paths": {
      "@tps/*": ["types/*"],
    }
  },
  "include": ["./lib/**/*", "types/*"]
}
