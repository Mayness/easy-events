{
  "name": "multi-events",
  "version": "1.3.2",
  "description": "A multiple events tool",
  "main": "dist/muti-events.js",
  "types": "types/event.d.ts",
  "jest": {
    "verbose": true,
    "browser": true,
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "events",
    "multi",
    "multi-events"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "rimraf coverage && jest",
    "coverage": "codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mayness/multi-events.git"
  },
  "author": "mayness",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mayness/multi-events/issues"
  },
  "homepage": "https://github.com/Mayness/multi-events#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.6.9",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3"
  }
}
